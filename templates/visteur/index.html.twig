{% extends 'base.html.twig' %}

{% block title %}Hello VisteurController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h3>Liste de Visiteur</h3>
<table>
    <tr>
        <td>Id</td>
        <td>Prenom</td>
        <td>Nom</td>
        <td>Code Postal</td>
        <td>Ville</td>


    </tr>
 {% for visiteur in visiteur %}
        <tr>
            <td>  {{ visiteur.id }}
</td>
<td>    {{ visiteur.prenom }}</td>
            <td>    {{ visiteur.nom }}</td>
             <td>    {{ visiteur.cp }}</td>
              <td>    {{ visiteur.ville }}</td>
        </tr>
    {% endfor %}
</table>



</div>
{% endblock %}

